<script src="assets/plugins/vu-debug/vu-debug.js"></script>
<div id="vu-debug-bar">
    <div id="vu-debug-header" class="">
        <div id="vu-debug-title" class="">
            &#9776;
            &nbsp;
            vuDebug
        </div>
        <div id="vu-debug-tabs" class="">
            <ul>
                {% for title,data in debug %}
                    <li>
                        <a href="#vu-debug-tab-{{ title }}">
                            {{ title }} <span class="vu-debug-count vu-debug-badge">0</span>
                        </a>
                    </li>
                {% endfor %}
            </ul>
        </div>
        <div id="vu-debug-controls" class="">
            <a href="#" id="vu-debug-request" title="Request">
                <span class="vu-debug-badge">{{ app.request }}</span>
                {{ app.pattern }}
            </a>
            <a href="#" id="vu-debug-memory" title="Memory usage">
                &#9881; {{ app.memory }}
            </a>
            <a href="#" id="vu-debug-close" title="Close vuDebug">
                &#10006;
            </a>
        </div>
        <div style="clear: both;"></div>
    </div>
    <div id="vu-debug-content" class="">
        {% for title,data in debug %}
            <div class="vu-debug-tab" id="vu-debug-tab-{{ title }}">
                <pre>{{ dump(data) }}</pre>
            </div>
        {% endfor %}
    </div>
</div>
